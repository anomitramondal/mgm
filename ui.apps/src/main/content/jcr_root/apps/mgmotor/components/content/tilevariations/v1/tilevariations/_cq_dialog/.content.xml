<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Tile Variations"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <content
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title to be displayed"
                                                fieldLabel="Title"
                                                name="./title"
                                                required="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="This is a summary description of the featured content"
                                                fieldLabel="Description"
                                                name="./description"
                                                required="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                    <format
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <formats jcr:primaryType="cq:WidgetCollection">
                                                            <paragraph
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Paragraph"
                                                                tag="p"/>
                                                            <h1
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 1"
                                                                tag="h1"/>
                                                            <h2
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 2"
                                                                tag="h2"/>
                                                            <h3
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 3"
                                                                tag="h3"/>
                                                            <h4
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 4"
                                                                tag="h4"/>
                                                            <h5
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 5"
                                                                tag="h5"/>
                                                            <h6
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 6"
                                                                tag="h6"/>
                                                        </formats>
                                                    </paraformat>
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <misctools
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                            <chars jcr:primaryType="nt:unstructured">
                                                                <ampersand
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#38;"/>
                                                                <endash
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#150;"/>
                                                                <emdash
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#151;"/>
                                                                <copyright
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#169;"/>
                                                                <registeredtrademark
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#174;"/>
                                                                <servicemark
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8480;"/>
                                                                <trademark
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8482;"/>
                                                                <centsign
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#162;"/>
                                                                <pound
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#163;"/>
                                                                <currencysign
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#164;"/>
                                                                <yensign
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#165;"/>
                                                                <euro
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8364;"/>
                                                                <left-pointingdoubleanglequotationmark
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#171;"/>
                                                                <rightanglequote
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#187;"/>
                                                                <dagger
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8224;"/>
                                                                <doubledagger
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8225;"/>
                                                                <feminineordinalindicator
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#170;"/>
                                                                <registration
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#174;"/>
                                                            </chars>
                                                        </specialCharsConfig>
                                                    </misctools>
                                                    <fullscreen
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <edit
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <findreplace
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <spellcheck
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <subsuperscript
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <styles
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <styles jcr:primaryType="cq:WidgetCollection">
                                                            <catbtn
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-promo-cta-button"
                                                                text="Promo CTA Button"/>
                                                            <catbtnwhite
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-promo-cta-button-white"
                                                                text="Promo CTA Button White"/>
                                                            <catbtnblue
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-promo-cta-button-blue"
                                                                text="Promo CTA Button Blue"/>
                                                            <secbtn
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-promo-cta-button-blue"
                                                                text="Secondary Button"/>
                                                            <cardcopy
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="card-copy strong.highlight"
                                                                text="Card Copy Strong Highlight"/>
                                                            <ctalink
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-openanaccount-CTA-link"
                                                                text="Open AC CTA Link"/>
                                                            <formheading
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="suntrust-form-heading suntrust-lookup-form-heading"
                                                                text="Form Heading"/>
                                                        </styles>
                                                    </styles>
                                                    <table
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                </rtePlugins>
                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                    <cui jcr:primaryType="nt:unstructured">
                                                        <inline
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                                <lists
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                    ref="lists"/>
                                                                <paraformat
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                                    ref="paraformat"/>
                                                                <styles
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="styles:getStyles:styles-pulldown"
                                                                    ref="styles"/>
                                                            </popovers>
                                                        </inline>
                                                        <dialogFullScreen
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                                <lists
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                    ref="lists"/>
                                                                <paraformat
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                                    ref="paraformat"/>
                                                                <styles
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="styles:getStyles:styles-pulldown"
                                                                    ref="styles"/>
                                                            </popovers>
                                                        </dialogFullScreen>
                                                        <tableEditOptions
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]"/>
                                                    </cui>
                                                </uiSettings>
                                                <htmlRules jcr:primaryType="nt:unstructured">
                                                    <serializer jcr:primaryType="nt:unstructured">
                                                        <config
                                                            jcr:primaryType="nt:unstructured"
                                                            idAttribMode="keep"/>
                                                    </serializer>
                                                    <docType jcr:primaryType="nt:unstructured">
                                                        <typeConfig
                                                            jcr:primaryType="nt:unstructured"
                                                            useSemanticMarkup="{Boolean}true"/>
                                                    </docType>
                                                </htmlRules>
                                            </description>
                                            <fontTheme
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                class="cq-dialog-dropdown-showhide"
                                                cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                fieldDescription="Sets the Font Theme"
                                                fieldLabel="Font Theme"
                                                name="./fontTheme">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="font-theme-white"/>
                                                    <dark
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Dark"
                                                        value="font-theme-dark"/>
                                                </items>
                                            </fontTheme>
                                            <cta
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        class="full-width"
                                                        composite="{Boolean}true"
                                                        fieldDescription="CTA Links"
                                                        fieldLabel="CTA Links">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            acs-commons-nested="JSON_STORE"
                                                            name="./cta">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <title
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Sets the title displayed in the button"
                                                                    fieldLabel="Title"
                                                                    name="./ctatitle"/>
                                                                <fontTheme
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    class="cq-dialog-dropdown-showhide"
                                                                    cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                                    fieldDescription="Sets the CTA Font Theme"
                                                                    fieldLabel="Font Theme"
                                                                    name="./ctaFontTheme">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <select
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Select"
                                                                            value=""/>
                                                                        <white
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="White"
                                                                            value="font-theme-white"/>
                                                                        <dark
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Dark"
                                                                            value="font-theme-dark"/>
                                                                    </items>
                                                                </fontTheme>
                                                                <url
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    allowBlank="{Boolean}false"
                                                                    fieldDescription="Sets the url target for the button "
                                                                    fieldLabel="URL"
                                                                    name="./url"
                                                                    rootPath="/content"/>
                                                                <urlParams
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Sets the link parameters applied to the url. e.g. param1=value1&amp;param2=value2"
                                                                    fieldLabel="Url Params"
                                                                    name="./urlParams"/>
                                                                <target
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    class="cq-dialog-dropdown-showhide"
                                                                    cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                                    fieldDescription="Sets the window target for the link"
                                                                    fieldLabel="Target"
                                                                    name="./target">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <self
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Default"
                                                                            value="_self"/>
                                                                        <blank
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Blank"
                                                                            value="_blank"/>
                                                                    </items>
                                                                </target>
                                                                <anchorTag
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Sets an anchor tag that’s applied to the link generated for the navigation item."
                                                                    fieldLabel="Anchor Tag"
                                                                    name="./anchorTag"/>
                                                                <theme
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                                    fieldDescription="Sets the theme applied to the link."
                                                                    fieldLabel="Theme"
                                                                    name="./theme"/>
                                                                <alignment
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    class="cq-dialog-dropdown-showhide"
                                                                    cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                                    fieldDescription="Sets the CTA Alignment"
                                                                    fieldLabel="Alignment"
                                                                    name="./ctaAlignment">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <right
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Right"
                                                                            value="text-right"/>
                                                                        <left
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Left"
                                                                            value="text-left"/>
                                                                        <center
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Center"
                                                                            value="text-center"/>
                                                                    </items>
                                                                </alignment>
                                                            </items>
                                                        </field>
                                                    </links>
                                                </items>
                                            </cta>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </content>
                    <appearance
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Appearance"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <background
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Background Image &amp; Options"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <topbordercolor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        class="cq-dialog-dropdown-showhide"
                                                        cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                        fieldDescription="Sets the thick top border color."
                                                        fieldLabel="Top Border Color"
                                                        name="./topbordercolor">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <select
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Select"
                                                                value=""/>
                                                            <orange
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Orange"
                                                                value="border-color-orange"/>
                                                            <green
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Green"
                                                                value="border-color-green"/>
                                                            <pink
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Pink"
                                                                value="border-color-pink"/>
                                                            <blue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Blue"
                                                                value="border-color-blue"/>
                                                            <yellow
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Yellow"
                                                                value="border-color-yellow"/>
                                                            <lightblue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Light Blue"
                                                                value="border-color-skyblue"/>
                                                        </items>
                                                    </topbordercolor>
                                                    <bgimage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./bgImage"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                                        multiple="{Boolean}false"
                                                        name="./file"
                                                        title="Upload Background Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                    <backgroundcolor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                        fieldDescription="Background Color"
                                                        fieldLabel="Background Color"
                                                        name="./bgcolor"/>
                                                </items>
                                            </background>
                                            <glymph
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Glymph Image &amp; Options"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <glymphimage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fieldLabel="Glymph Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./glymphImage"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                                        multiple="{Boolean}false"
                                                        name="./file"
                                                        title="Upload Glymph Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                    <alignment
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        class="cq-dialog-dropdown-showhide"
                                                        cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                                        fieldDescription="Sets the Glymph Alignment"
                                                        fieldLabel="Alignment"
                                                        name="./glymphAlignment">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <right
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Right"
                                                                value="pull-right"/>
                                                            <left
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Left"
                                                                value="pull-left"/>
                                                            <center
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Center"
                                                                value="pull-center"/>
                                                        </items>
                                                    </alignment>
                                                </items>
                                            </glymph>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </appearance>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
